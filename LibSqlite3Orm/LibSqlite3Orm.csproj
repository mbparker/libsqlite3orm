<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <TargetFramework>net9.0</TargetFramework>
        <LangVersion>latestmajor</LangVersion>
        <ImplicitUsings>enable</ImplicitUsings>
        <Nullable>disable</Nullable>
        <Configurations>Release;Debug</Configurations>
        <Platforms>AnyCPU</Platforms>
        <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
        <Title>Mathew Parker</Title>
        <Description>A cross platform, easy to use, Object Relational Mapper for SQLite 3</Description>
        <Copyright>2025, Mathew Parker</Copyright>
        <RepositoryUrl>https://github.com/mbparker/libsqlite3orm</RepositoryUrl>
        <PackageTags>Cross Platform, ORM, .NET, Sqlite</PackageTags>
        <Version>1.2.54</Version>
        <Company>Mathew Parker</Company>
    </PropertyGroup>

    <ItemGroup>
      <PackageReference Include="Autofac" Version="8.4.0" />
      <PackageReference Include="Newtonsoft.Json" Version="13.0.4" />
      <PackageReference Include="System.IO.Hashing" Version="9.0.9" />
      <PackageReference Include="SQLitePCLRaw.lib.e_sqlite3" Version="2.1.10" />
    </ItemGroup>

</Project>
